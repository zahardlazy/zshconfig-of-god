{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "template": "\u256d\u2500"
        },
        {
          "background": "#00BFFF",
          "background_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "foreground": "#000000",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#292929",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue0b1",
          "type": "text"
        },
        {
          "background": "#292929",
          "foreground": "#FF66CC",
          "foreground_templates": [
            "{{ if .Root }}#00FF00{{ end }}"
          ],
          "properties": {
            "style": "full",
            "trailing_diamond": "\ue0c6"
          },
          "style": "diamond",
          "template": "{{ .Path }} <{{ if .Root }}#CC0000{{ else }}#00BFFF{{ end }}>\ue0b1</> ",
          "type": "path"
        },
        {
          "background": "#292929",
          "foreground": "#D4E157",
          "style": "plain",
          "type": "git"
        },
        {
          "background": "#292929",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}",
            "{{ if eq \"Full\" .State.String }}#292929{{ end }}",
            "{{ if eq \"Charging\" .State.String }}#00FF00{{ end }}",
            "{{ if eq \"Discharging\" .State.String }}#FF0000{{ end }}"
          ],
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\ueb2d ",
            "discharging_icon": "\ueb2d ",
            "text": "\ue0b0"
          },
          "style": "powerline",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} \ue0b1",
          "type": "battery"
        },
        {
          "background": "#292929",
          "background_templates": [
            "{{ if gt .Code 0 }}#292929{{ end }}"
          ],
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}",
            "{{ if gt .Code 0 }}#DD0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "display_exit_code": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf00c{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#292929",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}<#FFFFFF>{{ if .Root }}\uf0e7{{ else }}\uf007{{ end }}</> <#FF0000>{{ .UserName }}</> <{{ if .Root }}#CC0000{{ else }}#00BFFF{{ end }}>\ue0b3</> ",
          "type": "session"
        },
        {
          "background": "#292929",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "properties": {
            "time_format": "15:04:05, _2"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }} \uf073 ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#00BFFF",
          "foreground_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "template": "\u2570\u2500"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "background": "#00BFFF",
          "background_templates": [
            "{{ if .Root }}#CC0000{{ end }}"
          ],
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": "\ue7ad",
          "trailing_diamond": "\ue0b4",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}